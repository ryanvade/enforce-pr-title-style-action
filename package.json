  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/webhooks": "^12.0.8",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.1",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
